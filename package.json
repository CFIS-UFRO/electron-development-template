{
  "name": "electron-development-template",
  "productName": "Electron Development Template",
  "description": "Template to develop GUI apps based on Electron",
  "main": "./out/main/index.js",
  "author": "Bryan Casanelli <bryancasanelli@gmail.com>",
  "port": 9826,
  "repository": "https://github.com/CFIS-UFRO/electron-development-template",
  "versions_repository": "https://github.com/CFIS-UFRO/versions",
  "version": "2025.4.10.0",
  "type": "module",
  "scripts": {
    "format": "prettier --plugin prettier-plugin-svelte --write .",
    "lint": "eslint --cache .",
    "start": "electron-vite preview",
    "dev": "electron-vite dev",
    "build": "electron-vite build",
    "postinstall": "electron-builder install-app-deps",
    "build:unpack": "npm run build && electron-builder --dir",
    "build:win": "npm run build && electron-builder --win zip --publish never",
    "build:mac": "npm run build && electron-builder --mac zip --publish never",
    "build:linux": "npm run build && electron-builder --linux zip --publish never",
    "publish": "node -r dotenv/config scripts/publish.js && git push && git push --tags"
  },
  "dependencies": {
    "@electron-toolkit/preload": "^3.0.1",
    "@electron-toolkit/utils": "^4.0.0",
    "@iconify/icons-mdi": "^1.2.48",
    "@iconify/svelte": "^4.2.0",
    "@tailwindcss/vite": "^4.0.17",
    "body-parser": "^2.2.0",
    "cors": "^2.8.5",
    "electron-log": "^5.3.3",
    "electron-store": "^10.0.1",
    "express": "^4.21.2",
    "i18next": "^24.2.3",
    "svelte-i18n": "^4.0.1",
    "tailwindcss": "^4.0.17"
  },
  "devDependencies": {
    "@electron-toolkit/eslint-config": "^2.0.0",
    "@electron-toolkit/eslint-config-prettier": "^3.0.0",
    "@sveltejs/vite-plugin-svelte": "^5.0.3",
    "electron": "^35.0.3",
    "electron-builder": "^25.1.8",
    "electron-vite": "^3.1.0",
    "eslint": "^9.23.0",
    "eslint-plugin-svelte": "^2.46.1",
    "prettier": "^3.5.3",
    "prettier-plugin-svelte": "^3.3.3",
    "svelte": "^5.25.3",
    "vite": "^6.2.3"
  }
}